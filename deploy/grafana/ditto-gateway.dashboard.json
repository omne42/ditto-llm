{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "9.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "id": 1,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum(rate(ditto_gateway_proxy_requests_total{job=~\"$job\"}[5m]))",
          "legendFormat": "requests/s",
          "refId": "A"
        }
      ],
      "title": "Proxy Requests",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum(rate(ditto_gateway_proxy_responses_total{job=~\"$job\",status=~\"5..\"}[5m])) / clamp_min(sum(rate(ditto_gateway_proxy_responses_total{job=~\"$job\"}[5m])), 1)",
          "legendFormat": "5xx rate",
          "refId": "A"
        }
      ],
      "title": "5xx Error Rate",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "id": 3,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(ditto_gateway_proxy_request_duration_seconds_bucket{job=~\"$job\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(ditto_gateway_proxy_request_duration_seconds_bucket{job=~\"$job\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ],
      "title": "Proxy Request Latency",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "id": 4,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum(rate(ditto_gateway_proxy_cache_hits_total{job=~\"$job\"}[5m])) / clamp_min(sum(rate(ditto_gateway_proxy_cache_lookups_total{job=~\"$job\"}[5m])), 1)",
          "legendFormat": "hit ratio",
          "refId": "A"
        }
      ],
      "title": "Proxy Cache Hit Ratio",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 16 },
      "id": 5,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (backend) (rate(ditto_gateway_proxy_backend_attempts_total{job=~\"$job\"}[5m]))",
          "legendFormat": "attempts {{backend}}",
          "refId": "A"
        },
        {
          "expr": "sum by (backend) (rate(ditto_gateway_proxy_backend_failures_total{job=~\"$job\"}[5m]))",
          "legendFormat": "failures {{backend}}",
          "refId": "B"
        }
      ],
      "title": "Backend Attempts/Failures",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 16 },
      "id": 6,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (backend) (ditto_gateway_proxy_backend_in_flight{job=~\"$job\"})",
          "legendFormat": "{{backend}}",
          "refId": "A"
        }
      ],
      "title": "Backend In-Flight",
      "type": "timeseries"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["ditto", "gateway"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": ".*", "value": ".*" },
        "hide": 0,
        "includeAll": false,
        "label": "job",
        "multi": false,
        "name": "job",
        "options": [],
        "query": {
          "query": "label_values(ditto_gateway_proxy_requests_total, job)",
          "refId": "StandardVariableQuery"
        },
        "refresh": 1,
        "regex": "",
        "type": "query",
        "datasource": "${DS_PROMETHEUS}"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "Ditto Gateway",
  "uid": "ditto-gateway",
  "version": 1
}
